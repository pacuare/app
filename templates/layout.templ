package templates
import "app.pacuare.dev/components/importmap"
import "app.pacuare.dev/components/styles"

templ layout(title string) {
	<!doctype html>
	<html>
		<head>
			<title>{ title }</title>
			@styles.Global()
			@importmap.ImportMap()
		</head>
		<body class="h-screen min-h-screen max-h-screen">
			<main>
			{ children... }
			</main>
		</body>
	</html>
}
