package templates
import "pacuare.dev/dash/templates/components"
import "pacuare.dev/dash/templates/styles"

templ layout(title string) {
	<html>
		<head>
			<title>{ title }</title>
			@styles.Global()
			@components.ImportMap()
		</head>
		<body>
			<main>
			{ children... }
			</main>
		</body>
	</html>
}
