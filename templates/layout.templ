package templates
import "dash.pacuare.dev/components/importmap"
import "dash.pacuare.dev/components/styles"

templ layout(title string) {
	<!doctype html>
	<html>
		<head>
			<title>{ title }</title>
			@styles.Global()
			@importmap.ImportMap()
		</head>
		<body>
			<main>
			{ children... }
			</main>
		</body>
	</html>
}
